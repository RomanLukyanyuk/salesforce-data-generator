{
  "name": "salesforce-data-generator",
  "version": "1.0.0",
  "description": "Web application for generating fake data for Salesforce organizations",
  "scripts": {
    "start": "npm run _server:run",
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail && npm run _server:build",
    "postinstall": "webpack --config config/webpack.prod.js --progress --profile --bail",
    "_start:dev": "concurrently --kill-others \"webpack-dev-server --config config/webpack.dev.js --inline --progress --port 3000\" \"npm run _server:run\"",
    "_start:prod": "npm start",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./server"
  },
  "keywords": [
    "salesforce",
    "angular"
  ],
  "author": "roman-lukyanyuk",
  "license": "ISC",
  "dependencies": {
    "@angular/common": "^2.4.3",
    "@angular/compiler": "^2.4.3",
    "@angular/core": "^2.4.3",
    "@angular/forms": "^2.4.3",
    "@angular/http": "^2.4.3",
    "@angular/material": "^2.0.0-beta.1",
    "@angular/platform-browser": "^2.4.3",
    "@angular/platform-browser-dynamic": "^2.4.3",
    "@angular/platform-server": "^2.4.3",
    "@angular/router": "^3.4.3",
    "@ngrx/core": "^1.2.0",
    "@ngrx/store": "^2.2.1",
    "core-js": "^2.4.1",
    "express": "^4.14.0",
    "hammerjs": "^2.0.8",
    "node-sass": "^4.3.0",
    "normalize-scss": "^6.0.0",
    "rxjs": "^5.0.3",
    "typescript": "^2.1.5",
    "webpack-merge": "^2.4.0",
    "zone.js": "^0.7.5"
  },
  "devDependencies": {
    "@types/body-parser": "0.0.33",
    "@types/express": "^4.0.34",
    "@types/hammerjs": "^2.0.34",
    "@types/node": "^6.0.60",
    "angular-router-loader": "^0.5.0",
    "angular2-template-loader": "^0.6.0",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "body-parser": "^1.15.2",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.26.0",
    "node-sass": "^4.2.0",
    "nodemon": "^1.11.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "sass-loader": "^4.1.1",
    "to-string-loader": "^1.1.5",
    "tslint": "^4.3.1",
    "typescript": "^2.1.4",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^2.3.1"
  }
}
